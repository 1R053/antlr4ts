export class XPathWildcardElement extends XPathElement {
	 constructor()  {
		super(XPath.WILDCARD);
	}

	@Override
	evaluate(final ParseTree t): Collection<ParseTree> {
		if ( invert ) return new ArrayList<ParseTree>(); // !* is weird but valid (empty)
		let kids: List<ParseTree> =  new ArrayList<ParseTree>();
		for (let c of Trees.getChildren(t)) {
			kids.add((ParseTree)c);
		}
		return kids;
	}
}
